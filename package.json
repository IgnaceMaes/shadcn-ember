{
  "name": "shadcn-ember-workspace",
  "version": "0.0.0",
  "private": true,
  "description": "shadcn-ember workspace",
  "repository": "https://github.com/IgnaceMaes/shadcn-ember",
  "license": "MIT",
  "author": "Ignace Maes",
  "scripts": {
    "dev": "pnpm --filter shadcn-ember-docs dev",
    "build": "pnpm --filter shadcn-ember-docs build",
    "lint": "pnpm --filter shadcn-ember-docs lint",
    "lint:fix": "pnpm --filter shadcn-ember-docs lint:fix",
    "dev:cli": "pnpm --filter shadcn-ember dev",
    "start:cli": "pnpm --filter shadcn-ember start:dev",
    "build:cli": "pnpm --filter shadcn-ember build",
    "registry:build-only": "pnpm --filter=shadcn-ember-docs registry:build",
    "registry:build": "pnpm --filter=shadcn-ember-docs registry:build && pnpm lint:fix",
    "test": "pnpm --filter shadcn-ember test"
  },
  "devDependencies": {
    "@vitest/ui": "^3.2.4",
    "prettier": "^3.7.3",
    "release-plan": "^0.17.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.0.0",
  "engines": {
    "node": ">= 20",
    "pnpm": ">= 10"
  }
}
