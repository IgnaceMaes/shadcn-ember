{
  "extends": "@ember/app-tsconfig",
  "include": ["app", "tests", "types"],
  "compilerOptions": {
    "allowJs": true,
    "paths": {
      "shadcn-ember-docs/tests/*": ["./tests/*"],
      "shadcn-ember-docs/*": ["./app/*"],
      "*": ["./types/*"],
      "@/*": ["./app/*"]
    },
    "types": [
      "ember-source/types",
      "@embroider/core/virtual",
      "vite/client",
      "unplugin-icons/types/ember"
    ],
    "erasableSyntaxOnly": true,
    "isolatedModules": true
  }
}
